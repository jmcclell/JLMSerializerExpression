{
    "name": "jlm/serializer-expression",
    "type": "library",
    "description": "Allows you to exclude fields at runtime via expressions using the Symfony expression language",
    "keywords": ["serialization", "serialize", "jms serializer", "exclusion"],
    "homepage": "http://github.com/jmcclell/JLMSerializerExpression",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason McClellan",
            "email": "jason@jasonmcclellan.io"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "jms/serializer": "~0.11",                
        "symfony/expression-language": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": { "JLM\\SerializerExpression": "src/" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
